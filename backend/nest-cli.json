{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "morallis": {
      "type": "library",
      "root": "libs/morallis",
      "entryFile": "index",
      "sourceRoot": "libs/morallis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/morallis/tsconfig.lib.json"
      }
    },
    "eth": {
      "type": "library",
      "root": "libs/eth",
      "entryFile": "index",
      "sourceRoot": "libs/eth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/eth/tsconfig.lib.json"
      }
    }
  }
}